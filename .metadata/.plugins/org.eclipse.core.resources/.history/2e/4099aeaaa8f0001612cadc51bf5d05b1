package controller;

public class Song {
	
	public String name;
	public String artist;
	public String album;
	public String year;
	
	public Song(String name, String artist, String album, String year){
		this.name = name;
		this.artist = artist;
		this.album = album;
		this.year = year;
	}
	
	public Song(String name, String artist){
		this(name,artist,"","");
	}
	public String toString(){
		return "Song: "+name+"\nArtist: "+artist+"\nAlbum: "+album+"\nYear:"+year;
		
	}

}
